{
    "manifest_version": 2,

    "name": "AllNewsIsFakeNews Plugin",
    "description": "This extension will change all news to fake news",
    "version": "1.0",

    "browser_action": {
     "default_icon": "icon.jpg",
     "default_popup": "popup.html"
    },
    "permissions":[
     "activeTab"
     ],

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
